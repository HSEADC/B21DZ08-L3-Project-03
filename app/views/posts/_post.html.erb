<div id="<%= dom_id post %>">
  <p>
    <strong>Post title:</strong>
    <%= post.post_title %>
  </p>

  <p>
    <strong>Pic:</strong>
    <%= post.pic %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= post.description %>
  </p>

  <p>
    <strong>Link:</strong>
    <%= post.link %>
  </p>

  <p>
    <strong>Profile:</strong>
    <%= post.profile_id %>
  </p>

</div>
